   "source": [
    "grouped_df = df.groupby('Year').agg({\n",
    "    'CO2 Emissions (Tons/Capita)': 'mean',\n",
    "    'Avg Temperature (C)': 'mean',\n",
    "    'Extreme Weather Events': 'first'  \n",
    "}).reset_index()\n",
    "co2_extreme = px.scatter(grouped_df, y=\"CO2 Emissions (Tons/Capita)\", x=\"Avg Temperature (C)\", color='Extreme Weather Events',color_continuous_scale='blues')\n",
    "co2_extreme.show()"
   ]
  },
