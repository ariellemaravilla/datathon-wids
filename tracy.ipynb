    "temp_map = px.choropleth(df, \n",
    "                    locations = 'Country', \n",
    "                    locationmode = 'country names', \n",
    "                    color = 'Avg Temperature (C)',\n",
    "                    hover_name = 'Country',\n",
    "                    animation_frame = 'Year',\n",
    "                    range_color=[min(df['Avg Temperature (C)']), max(df['Avg Temperature (C)'])],\n",
    "                    projection = 'natural earth',\n",
    "                    title = 'Average Temperature (C) by Country',\n",
    "                    width = 1000,\n",
    "                    height = 800,\n",
    "                    color_continuous_scale='YlOrRd' )\n",
    "\n",
    "temp_map.show()"
   ]
