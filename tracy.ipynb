    "#Checking number of Countries\n",
    "\n",
    "country_num = df['Country'].value_counts()\n",
    "print(country_num)\n",
    "\n",
    "#Creating a dictionary for Country to Continent\n",
    "\n",
    "country_to_continent = {\"Asia\": [\"Indonesia\", \"Russia\", \"India\", \"China\", \"Japan\"],\n",
    "    \"North America\": [\"USA\", \"Canada\", \"Mexico\"],\n",
    "    \"South America\": [\"Brazil\", \"Argentina\"],\n",
    "    \"Europe\": [\"Russia\", \"France\", \"UK\", \"Germany\"],\n",
    "    \"Africa\": [\"South Africa\"],\n",
    "    \"Oceania\": [\"Australia\"]}\n",
    "\n",
    "def get_continent(country):\n",
    "    for continent, countries in country_to_continent.items():\n",
    "        if country in countries:\n",
    "            return continent\n",
    "\n",
    "    return \"Unknown\"\n",
    "        \n",
    "df['Continent'] = df['Country'].apply(get_continent)\n",
    "df.head()"
   ]
  },
